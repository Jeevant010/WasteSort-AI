{
  "buildCommand": "cd app && npm install && npm run build",
  "outputDirectory": "app/dist/waste-sort-ai/browser",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|json))",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
